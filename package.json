{
  "name": "discordbot",
  "version": "1.0.0",
  "description": "discord bot",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./dist/index.js",
    "build-dist": "./node_modules/typescript/bin/tsc",
    "superbuild": "yarn build-dist && yarn push && yarn seed && yarn start",
    "watch": "nodemon -e ts --watch src --exec 'yarn build-dist && yarn start'",
    "test": "jest",
    "seed": "prisma db seed",
    "push": "prisma db push",
    "format": "prisma format",
    "lint": "eslint . --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KirbyPaint/psychosomatic.git"
  },
  "author": "KirbyPaint",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KirbyPaint/psychosomatic/issues"
  },
  "homepage": "https://github.com/KirbyPaint/psychosomatic#readme",
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "axios": "^1.4.0",
    "chalk": "4.1.2",
    "discord.js": "^14.11.0",
    "dotenv": "^16.1.4",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "nodemon": "^2.0.22",
    "openai": "^3.2.1",
    "sqlite3": "^5.1.6",
    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
