    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "babel-jest": "^29.5.0",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "prisma": "^4.15.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.7.0",
    "typescript": "^5.1.3",
    "vitest": "^0.32.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "packageManager": "yarn@3.5.0"
